  <img src={`${currentUser.avatar}`} alt=\"user profile\" />\n              </span>\n              <span className=\"user-name\" id=\"user-name\">\n                <span>{currentUser.name}</span>\n                <small>Go to your profile</small>\n              </span>\n            </Link>\n          </li>\n        )}\n\n        {/* Setting Color mode */}\n        <li>\n          <div className=\"popup-item-left\">\n            <span className=\"popup-item-left__icon\">\n              <FaRegSun />\n            </span>\n            <span className=\"popup-item-left__name\">Apperance</span>\n          </div>\n          <div className=\"popup-item-right\">\n            <ButoonColorMode />\n          </div>\n        </li>\n        {/* Setting Language */}\n        <li>\n          <div className=\"popup-item-left\">\n            <span className=\"popup-item-left__icon\">\n              <MdLanguage />\n            </span>\n            <span className=\"popup-item-left__name\">Language</span>\n          </div>\n          <div className=\"popup-item-right\">\n            <Button variant=\"outlined\">\n              <FaChevronRight />\n            </Button>\n          </div>\n        </li>\n        {/* Setting Privacy */}\n        <li>\n          <div className=\"popup-item-left\">\n            <span className=\"popup-item-left__icon\">\n              <AiOutlineSetting />\n            </span>\n            <span className=\"popup-item-left__name\">Settings & Privacies</span>\n          </div>\n          <div className=\"popup-item-right\">\n            <Button variant=\"outlined\">\n              <FaChevronRight />\n            </Button>\n          </div>\n        </li>\n        {/* Logout User Account*/}\n        {currentUser && (\n          <li onClick={onLogout}>\n            <div className=\"popup-item-left\">\n              <span className=\"popup-item-left__icon\">\n                <FiLogOut />\n              </span>\n              <span className=\"popup-item-left__nam